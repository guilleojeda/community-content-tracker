{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "build:frontend": "cd ../frontend && npm run build",
    "watch": "tsc -w",
    "test": "jest",
    "test:all": "cd ../.. && npm run typecheck",
    "cdk": "cdk",
    "synth": "npm run build:frontend && cdk synth",
    "diff": "cdk diff",
    "deploy:dev": "npm run test:all && npm run build:frontend && cdk deploy --all --context environment=dev",
    "deploy:staging": "npm run test:all && npm run build:frontend && cdk deploy --all --context environment=staging",
    "deploy:prod": "npm run test:all && npm run build:frontend && cdk deploy --all --context environment=prod",
    "destroy:dev": "cdk destroy --all --context environment=dev",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk": "^2.1030.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.901.0",
    "aws-cdk-lib": "^2.219.0",
    "constructs": "^10.0.0"
  }
}

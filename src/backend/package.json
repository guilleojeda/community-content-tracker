{
    "name": "@aws-community-hub/backend",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "dev": "ts-node-dev --respawn --transpile-only src/local-server.ts",
        "db:migrate": "node --require ./scripts/configureDotenv.js --require ./scripts/ensureDatabaseUrl.js ../../node_modules/.bin/node-pg-migrate up --database-url-var DATABASE_URL --migrations-dir migrations",
        "migrate:up": "node --require ./scripts/configureDotenv.js --require ./scripts/ensureDatabaseUrl.js ../../node_modules/.bin/node-pg-migrate up --database-url-var DATABASE_URL --migrations-dir migrations",
        "migrate:down": "node --require ./scripts/configureDotenv.js --require ./scripts/ensureDatabaseUrl.js ../../node_modules/.bin/node-pg-migrate down --database-url-var DATABASE_URL --migrations-dir migrations",
        "migrate:create": "../../node_modules/.bin/node-pg-migrate create --migrations-dir migrations",
        "db:seed": "ts-node src/database/seeds/seed.ts",
        "seed": "ts-node src/database/seeds/seed.ts",
        "bootstrap:admin": "ts-node scripts/bootstrap-admin.ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@aws-community-hub/shared": "*",
        "@aws-sdk/client-bedrock-runtime": "^3.899.0",
        "@aws-sdk/client-cloudwatch": "^3.899.0",
        "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
        "@aws-sdk/client-dynamodb": "^3.899.0",
        "@aws-sdk/client-lambda": "^3.899.0",
        "@aws-sdk/client-secrets-manager": "^3.0.0",
        "@aws-sdk/client-sqs": "^3.899.0",
        "@aws-sdk/util-dynamodb": "^3.899.0",
        "@types/axios": "^0.9.36",
        "@types/express": "^5.0.3",
        "@types/qrcode": "^1.5.5",
        "aws-sdk": "^2.1691.0",
        "axios": "^1.12.0",
        "dotenv": "^16.4.5",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.0",
        "jwks-rsa": "^3.1.0",
        "node-pg-migrate": "^8.0.3",
        "pg": "^8.11.0",
        "pgvector": "^0.1.0",
        "qrcode": "^1.5.0",
        "rss-parser": "^3.13.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@testcontainers/postgresql": "^10.0.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/pg": "^8.10.0",
        "aws-sdk-client-mock": "^4.1.0",
        "aws-sdk-client-mock-jest": "^4.1.0",
        "pg-mem": "^2.9.1",
        "ts-node": "^10.9.0",
        "ts-node-dev": "^2.0.0"
    }
}

{
    "name": "@aws-community-hub/backend",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "npm --prefix ../shared run build",
        "build": "tsc",
        "lint": "eslint . --ext .ts --max-warnings=0",
        "pretest": "npm --prefix ../shared run build",
        "test": "jest --detectOpenHandles",
        "test:watch": "jest --watch",
        "pretest:coverage": "npm --prefix ../shared run build",
        "test:coverage": "jest --coverage --detectOpenHandles",
        "dev": "ts-node-dev --respawn --transpile-only src/local-server.ts",
        "db:migrate": "node ./scripts/run-db-migrate.js up",
        "migrate:up": "node ./scripts/run-db-migrate.js up",
        "migrate:down": "node ./scripts/run-db-migrate.js down",
        "migrate:create": "node ./scripts/run-db-migrate.js create",
        "db:seed": "ts-node src/database/seeds/seed.ts",
        "seed": "ts-node src/database/seeds/seed.ts",
        "bootstrap:admin": "ts-node scripts/bootstrap-admin.ts",
        "pretypecheck": "npm --prefix ../shared run build",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@aws-community-hub/shared": "*",
        "@aws-sdk/client-bedrock-runtime": "^3.899.0",
        "@aws-sdk/client-cloudwatch": "^3.899.0",
        "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
        "@aws-sdk/client-dynamodb": "^3.899.0",
        "@aws-sdk/client-lambda": "^3.899.0",
        "@aws-sdk/client-secrets-manager": "^3.0.0",
        "@aws-sdk/client-ses": "^3.929.0",
        "@aws-sdk/client-sqs": "^3.899.0",
        "@aws-sdk/util-dynamodb": "^3.899.0",
        "aws-xray-sdk-core": "^3.10.3",
        "@types/axios": "^0.9.36",
        "@types/express": "^5.0.3",
        "@types/qrcode": "^1.5.5",
        "axios": "^1.12.0",
        "dotenv": "^16.4.5",
        "express": "^5.2.1",
        "ioredis": "^5.8.1",
        "jsonwebtoken": "^9.0.0",
        "jwks-rsa": "^3.1.0",
        "node-pg-migrate": "^8.0.4",
        "pg": "^8.11.0",
        "pgvector": "^0.1.0",
        "qrcode": "^1.5.0",
        "rss-parser": "^3.13.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@testcontainers/postgresql": "^10.0.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/pg": "^8.10.0",
        "pg-mem": "^2.9.1",
        "ts-node": "^10.9.0",
        "ts-node-dev": "^2.0.0"
    }
}

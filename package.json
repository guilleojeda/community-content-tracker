{
    "name": "aws-community-content-hub",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
      "src/*"
    ],
    "scripts": {
      "build": "npm run build --workspaces",
      "test": "npm run test --workspaces",
      "lint": "npm run lint --workspaces",
      "typecheck": "npm run typecheck --workspaces",
      "dev:backend": "npm run dev --workspace=src/backend",
      "dev:frontend": "npm run dev --workspace=src/frontend",
      "db:migrate": "npm run migrate --workspace=src/backend",
      "db:seed": "npm run seed --workspace=src/backend"
    },
    "devDependencies": {
      "@types/node": "^20.0.0",
      "typescript": "^5.0.0",
      "prettier": "^3.0.0",
      "eslint": "^8.0.0",
      "@typescript-eslint/parser": "^6.0.0",
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "jest": "^29.0.0",
      "@types/jest": "^29.0.0",
      "ts-jest": "^29.0.0"
    },
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=9.0.0"
    }
  }